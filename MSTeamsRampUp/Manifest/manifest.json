{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.3/MicrosoftTeams.schema.json",
  "manifestVersion": "1.3",
  "version": "1.0.2",
  "id": "f8240f90-6212-42e9-9419-73f0f6e02fe8",
  "packageName": "teamsrampup",
  "developer": {
    "name": "Rick Van Rousselt",
    "websiteUrl": "https://www.rickvanrousselt.com",
    "privacyUrl": "https://www.rickvanrousselt.com",
    "termsOfUseUrl": "https://www.rickvanrousselt.com"
  },
  "icons": {
    "color": "color.png",
    "outline": "outline.png"
  },
  "name": {
    "short": "Rampup",
    "full": "Getting started with development"
  },
  "description": {
    "short": "Getting started",
    "full": "getting started"
  },
  "accentColor": "#8A038A",
  "staticTabs": [
    {
      "entityId": "EnterGuidHere",
      "name": "MyStaticTab",
      "contentUrl": "https://localhost:44314/",
